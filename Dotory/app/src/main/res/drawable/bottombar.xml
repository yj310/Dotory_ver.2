<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- solid -->
    <item>
        <shape>
            <padding android:top="1dp" android:right="0dp" android:bottom="0dp" android:left="0dp" />
            <corners android:radius="0dp" />
