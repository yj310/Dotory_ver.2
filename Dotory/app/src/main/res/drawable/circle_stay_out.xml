<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- solid -->
    <item>
        <shape>
            <padding
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp"
                android:left="2dp" />
            <corners android:radius="200dp" />
            <solid android:color="@color/colorTextDefault" />
        </shape>
    </item>



    <!-- Background -->
    <item>
        <shape>
            <solid android:color="#ffffff" />
            <corners android:radius="200dp" />
        </shape>
    </item>
</layer-list>